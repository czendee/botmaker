{
	"creadata": {
    "run": {
      "expressions": [
        "create datasource with name x",
        "create datasource new x"
      ],
      "entities": [
        {
          "type": "trim",
          "name": "nuevodata",
          "conditions": [
            {
              "type": "after",
              "from": "name"
            },
            {
              "type": "after",
              "from": "new"
            }            
          ]
        }
      ]
    }
  },
	"creaapi": {
    "run": {
      "expressions": [
        "create api x data y",
        "create api x add data y",
        "add api x  data y"
      ],
      "entities": [
        {
          "type": "trim",
          "name": "elapi",
          "conditions": [
            {
              "type": "between",
              "from": "api",
              "to":   "data"
            },
            {
              "type": "between",
              "from": "api",
              "to"  : "add"
            }
          ]
        },
        {
          "type": "trim",
          "name": "eldata",
          "conditions": [
            {
              "type": "after",
              "from": "data"
            }
          ]
        }        
      ]
    }
  },
