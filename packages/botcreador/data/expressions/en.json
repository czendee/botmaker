{
	"creabot": {
    "run": {
      "expressions": [
        "create bot x",
        "create bot with name x",
        "bot new x"
      ],
      "entities": [
        {
          "type": "trim",
          "name": "nuevobot",
          "conditions": [
            {
              "type": "after",
              "from": "bot"
            },
            {
              "type": "after",
              "from": "name"
            },
            {
              "type": "after",
              "from": "new"
            }
          ]
        }
      ]
    }
  },
	"createmabot": {
    "run": {
      "expressions": [
        "bot x add y tema ",
        "add y tema   into x bot"
      ],
      "entities": [
        {
          "type": "trim",
          "name": "elbot",
          "conditions": [
            {
              "type": "between",
              "from": "bot",
              "to":   "add"
            },
            {
              "type": "between",
              "from": "into",
              "to"  : "bot"
            }
          ]
        },
        {
          "type": "trim",
          "name": "eltema",
          "conditions": [
            {
              "type": "between",
              "from": "add",
              "to":   "tema"
            }
          ]
        }        
      ]
    }
  },
	"activabot": {
    "run": {
      "expressions": [
        "start bot x",
        "start bot with name x",
        "start new  bot x"
      ],
      "entities": [
        {
          "type": "trim",
          "name": "nuevobot",
          "conditions": [
            {
              "type": "after",
              "from": "bot"
            },
            {
              "type": "after",
              "from": "name"
            }
          ]
        }
      ]
    }
  },
  
